		"start": "probot run ./index.js",
		"test": "jest",
		"test:watch": "jest --watch --notify --notifyMode=change --coverage"
	},
	"dependencies": {
		"probot": "9.3.4"
	},
	"devDependencies": {
		"jest": "24.9.0",
		"nodemon": "^1.19.2",
		"smee-client": "1.1.0"
	},
	"engines": {
		"node": ">= 9"
	},
	"jest": {
		"testEnvironment": "node"
	},
	"nodemonConfig": {
		"exec": "npm start",
		"watch": [
			".env",
			"."
		]
	}
}